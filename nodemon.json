{
  "events": {
    "restart": "node ./scripts/clean-console"
  },
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "coverage"
  ],
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "debug.js",
    "src/**/*.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json"
}
