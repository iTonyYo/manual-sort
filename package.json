{
  "main": "index.js",
  "files": [
    "index.js",
    "esm/**/*.js",
    "umd/**/*.js",
    "src/**/*.js"
    "umd-sync.js",
    "manual-sort.js",
    "manual-sort-sync.js"
  ],
  "scripts": {
    "start": "npx nodemon ./debug.js --config ./nodemon.json -q",
    "clean": "npx babel-node ./scripts/clean",
    "reprepare": "npx babel-node ./scripts/re-prepare",
    "build": "npx babel-node ./scripts/clean-built && yarn babel-build-sync-src && yarn babel-build-async-src && yarn webpack-build",
    "babel-build-sync-src": "npx babel -o ./esm-sync.js manual-sort-sync.js",
    "babel-build-async-src": "npx babel -o ./esm.js manual-sort.js",
    "test": "npx nyc mocha --require @babel/register --no-opts --no-package ./*-test.js",
    "coverage": "npx nyc report --reporter=text-lcov | npx coveralls",
    "lint": "npx eslint --cache ./src/**/*.js",
    "license": "npx babel-node ./scripts/license",
    "changelog": "npx auto-changelog -p"
  },
  "keywords": [
    "手动排序",
    "manualSort",
    "排序",
    "sort",
