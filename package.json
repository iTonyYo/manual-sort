{
  "main": "index.js",
  "scripts": {
    "start": "npx nodemon ./debug.js --config ./nodemon.json -q",
    "build": "node ./scripts/clean && npm run babel-build-sync-src && npm run babel-build-async-src && npm run webpack-build",
    "babel-build-sync-src": "npx babel -o ./esm-sync.js manual-sort-sync.js",
    "babel-build-async-src": "npx babel -o ./esm.js manual-sort.js",
    "webpack-build": "npx webpack",
    "test": "npx nyc mocha ./test.babel.js ./*-test.js --opts ./mocha.opts",
    "coverage": "npx nyc report --reporter=text-lcov | npx coveralls",
    "lint": "npx eslint --cache manual-sort.js manual-sort-sync.js",
    "license": "node ./scripts/license"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm run test"
    ]
  },
  "keywords": [
    "manualSort",
    "sort",
    "array",
    "modules",
    "util",
    "utilities",
    "library"
  ],
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.1.3",
    "license-checker": "^24.0.1",
    "lint-staged": "^8.0.4",
    "make-dir": "^1.3.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.5",
    "nyc": "^13.1.0",
    "trash": "^4.3.0",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2",
    "write-file-atomic": "^2.3.0"
  },
  "name": "manual-sort",
  "version": "0.0.0",
  "description": "手动排序数组。",
  "author": {
    "name": "iyowei",
    "email": "swzyocowboy@icloud.com",
    "url": "https://github.com/iTonyYo"
  },
  "license": "MIT"
}
