    "babel-plugin-add-module-exports": "^1.0.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "execa": "^1.0.0",
    "husky": "^1.3.1",
    "license-checker": "^25.0.1",
